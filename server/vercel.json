{
  "version": 2,
  "builds": [{ "src": "index.js", "use": "@vercel/node" }],
  "routes": [
    { "src": "/administrator/(.*)", "dest": "/index.js" },
    { "src": "/customer/(.*)", "dest": "/index.js" },
    { "src": "/authentication/(.*)", "dest": "/index.js" },
    { "src": "/(.*)", "dest": "/index.js" }
  ]
}
